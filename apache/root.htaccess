DirectoryIndex index.html index.txt index.cgi .ht

AddCharset UTF-8 txt

Header always set Cache-Control "private, no-store, no-transform"
Header always set Strict-Transport-Security "max-age=15724800"
Header always set X-Frame-Options "DENY"
Header always set X-Robots-Tag "noindex, noarchive, nosnippet, notranslate, noimageindex, noodp"
